{
    "root": {
        "prototype": "montage/core/meta/module-object-descriptor",
        "properties": {
            "name": "PlotSummary",
            "propertyDescriptors": [
                {"@": "movie"},
                {"@": "summary"}
            ],
            "objectDescriptorModule": {
                "%": "spec/data/logic/model/plot-summary.mjson"
            },
            "exportName": "PlotSummary",
            "module": {
                "%": "spec/data/logic/model/plot-summary"
            }
        }
    },


    "movie_reference": {
        "prototype": "montage/core/meta/object-descriptor-reference",
        "properties": {
            "valueReference": {
                "objectDescriptorName": "Category",
                "prototypeName": "Category",
                "objectDescriptorModule": {"%": "logic/model/descriptors/category.mjson"}
            }
        }
    },

    "movie": {
        "prototype": "montage/core/meta/property-descriptor",
        "properties": {
            "name": "movie",
            "valueDescriptor": {
                "@": "movie_reference",
                "_comment": "This cannot be object notation because it creates a circular reference with movie.mjson"
            }
        }
    },

    "summary": {
        "prototype": "montage/core/meta/property-descriptor",
        "properties": {
            "name": "summary",
            "valueType": "string"
        }
    }
}
